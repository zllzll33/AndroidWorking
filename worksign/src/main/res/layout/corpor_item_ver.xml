<?xml version="1.0" encoding="utf-8"?>
<com.luofangyun.shangchao.ui.SwipeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
                                            android:layout_width="match_parent"
                                            android:layout_height="40dp"
                                            android:id="@+id/swipelayout"
    app:clickToClose="true"
                                        >
    <LinearLayout
        android:layout_width="60dp"
        android:layout_height="40dp"
        android:orientation="horizontal"
        android:id="@+id/ll_delete">
        <TextView
            android:id="@+id/corpor_delete"
            android:layout_width="60dp"
            android:layout_height="40dp"
            android:textColor="@color/white"
            android:background="#f00"
            android:gravity="center"
            android:text="删除"/>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/address_empt_fl1"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="@drawable/rectangle_up_down"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:padding="8dp">
        <ImageView
            android:id="@+id/corpor_iv"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@mipmap/grade"/>
        <TextView
            android:id="@+id/corpor_tv"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="13dp"
            android:layout_toRightOf="@+id/address_grade"
            android:layout_weight="1"
            android:text="wqrwrwqr"
            android:textColor="@color/black"
            android:textSize="15dp"/>

        <ImageView
            android:id="@+id/corpor_right_iv"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:background="@mipmap/to_right"/>
    </LinearLayout>


</com.luofangyun.shangchao.ui.SwipeLayout>